        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.trackworktime.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.trackworktime.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="TextViewLikeEditText">
